@model AgendamentoModel
@{
    Layout = "_Layout";
}

<div class="text-center">
    <h3>Realmente deseja apagar o Agendamento (@Model.NameGameOrConsole)?</h3>
    <a role="button" class="btn btn-success" asp-controller="Agendamento" asp-route-id="@Model.Id" asp-action="Delete">Deletar</a>
    <a role="button" class="btn btn-danger" asp-controller="Agendamento" asp-action="Index">Cancelar</a>
</div>

