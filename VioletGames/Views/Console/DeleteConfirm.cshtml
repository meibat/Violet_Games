@model ConsoleModel
@{
    Layout = "_Layout";
}

<nav>
    <div class="text-center">
        <h3>Realmente deseja apagar o Console (@Model.Name)?</h3>
        <a role="button" class="btn btn-success" asp-controller="Console" asp-route-id="@Model.Id" asp-action="Delete">Deletar</a>
        <a role="button" class="btn btn-danger" asp-controller="Console" asp-action="Index">Cancelar</a>
    </div>
</nav>

